  \bxjy@default@setfalse}

%% \bxjy@set@genfam
\def\bxjy@set@genfam{%
  \bxjy@okfalse
  \edef\bxjy@tmpa{\f@family}%
  \edef\bxjy@tmpb{\sfdefault}%
  \ifx\bxjy@tmpa\bxjy@tmpb \bxjy@oktrue \fi
  \edef\bxjy@tmpb{\ttdefault}%
  \ifx\bxjy@tmpa\bxjy@tmpb \bxjy@oktrue \fi
  \edef\bxjy@genfam{\ifbxjy@ok \gtdefault \else \mcdefault \fi}}

