      \else
        \kern-\CJKboldshift\hb@xt@\z@{\hss\symbol{#1}}%
        \kern2\dimexpr\CJKboldshift\relax
        \hb@xt@\z@{\hss\symbol{#1}}\kern-\CJKboldshift\relax
      \fi
    \fi}%
}

